# notebooks/01_data_inspection.ipynb
import pandas as pd
import numpy as np
import matplotlib.pyplot as plt
import seaborn as sns

# Load the sample data
df = pd.read_excel("data/raw/Sample Data 2.xlsx")

# Basic inspection
print("Dataset Shape:", df.shape)
print("\nColumn Names:")
print(df.columns.tolist())
print("\nData Types:")
print(df.dtypes)
print("\nFirst 5 rows:")
print(df.head())

# Check for missing values
print("\nMissing Values:")
print(df.isnull().sum())

# Date range check
df['Date of Shipment'] = pd.to_datetime(df['Date of Shipment'], format='%d/%m/%Y', errors='coerce')
print(f"\nDate Range: {df['Date of Shipment'].min()} to {df['Date of Shipment'].max()}")

# Sample Goods Description analysis
print("\nSample Goods Descriptions:")
print(df['Goods Description'].head(10))

# Unit variations
print("\nUnit variations:")
print(df['Unit'].value_counts())